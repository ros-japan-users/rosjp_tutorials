<launch>
    <include file="$(find learning_tf)/launch/start_demo1.launch"/>

    <node name="turtle2_tf_broadcaster" pkg="learning_tf" type="turtle_tf_broadcaster.py"
          respawn="false" output="screen">
      <param name="turtle" type="string" value="turtle2"/>
    </node>

    <node name="turtle_tf_listener3" pkg="learning_tf" type="turtle_tf_listener3.py"
          respawn="false" output="screen">
      <param name="leader_tf" type="string" value="/carrot1"/>
    </node>

    <node name="fixed_tf_broadcaster" pkg="learning_tf" type="fixed_tf_broadcaster.py"
          respawn="false" output="screen"/>
  </launch>
